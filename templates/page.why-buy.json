        "1647738182370ae00a-0": {
          "type": "text_block",
          "settings": {
            "enable_image": true,
            "image": "shopify:\/\/shop_images\/account.png",
            "image_width": 60,
            "title": "Free Tech Support",
            "text": "<p>Need help? Our NY-based techs are available 24\/7 for all products — new and old — via email, SMS, chat, phone, and carrier pigeon. <\/p>",
            "button_label": "learn more",
            "button_link": "https:\/\/www.vidlogix.com\/pages\/tech-support"
          }
        },
        "1647738182370ae00a-2": {
          "type": "text_block",
          "settings": {
            "enable_image": true,
            "image": "shopify:\/\/shop_images\/account.png",
            "image_width": 60,
            "title": "Free Shipping",
            "text": "<p>We offer free shipping and easy returns on all orders over $49. <\/p>",
            "button_label": "learn more",
            "button_link": "https:\/\/www.vidlogix.com\/pages\/shipping"
          }
        },
        "1647738182370ae00a-1": {
          "type": "text_block",
          "settings": {
            "enable_image": true,
            "image": "shopify:\/\/shop_images\/account.png",
            "image_width": 60,
            "title": "60-Day Free Returns",
            "text": "<p>Take some time, make sure it's the right product for you. If you don't love it, send it back<\/p>",
            "button_label": "learn more",
            "button_link": "https:\/\/www.vidlogix.com\/pages\/returns"
          }
        },
        "c08dc1cc-1175-417c-9cca-9b1b4a1490b8": {
          "type": "text_block",
          "settings": {
            "enable_image": true,
            "image": "shopify:\/\/shop_images\/account.png",
            "image_width": 60,
            "title": "60-day price match",
            "text": "<p>We'll match the price shown on any US-based retailer's website for the <strong>identical in-stock product<\/strong> within 60-days of purchase. <\/p>",
            "button_label": "see full details",
            "button_link": "https:\/\/www.vidlogix.com\/pages\/price-match"
          }
        }
      },
